@model SchoolSys.DataViewModels.RegisterViewModel
@{
    ViewData["Title"] = "InsertUser";
}

<h1>InsertUser</h1>

<form method="post">
    <label asp-for="@Model.Email"></label>
    <input asp-for="@Model.Email" />
    <span asp-validation-for="@Model.Email"></span>

    <label asp-for="@Model.Password"></label>
    <input type="password" asp-for="@Model.Password" />
    <span asp-validation-for="@Model.Password"></span>

    <input type="submit" value="register" />


</form>

